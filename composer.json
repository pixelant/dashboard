{
  "name": "pixelant/dashboard",
  "type": "typo3-cms-extension",
  "description": "Customizable dashboard which can display dashboard widgets.",
  "license": ["GPL-2.0+"],
  "keywords": ["TYPO3 CMS", "Dashboard"],
  "require": {
    "typo3/cms-core": "^8.7",
    "typo3/cms-backend": "^8.7",
    "typo3/cms-extbase": "^8.7",
    "typo3/cms-lang": "^8.7",
    "typo3/cms-frontend": "^8.7",
    "typo3/cms-fluid": "^8.7",
    "typo3/cms-sys-action": "^8.7"
  },
  "replace": {
    "dashboard": "self.version"
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "dashboard"
    }
  },
  "autoload": {
    "psr-4": {
      "Pixelant\\Dashboard\\": "Classes/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pixelant\\Dashboard\\Tests\\": "Tests/"
    }
  }
}
